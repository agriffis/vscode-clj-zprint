{
    "name": "vscode-clj-zprint",
    "displayName": "vscode-clj-zprint",
    "description": "Wrapper for clj-zprint.",
    "version": "0.0.1",
    "publisher": "rflagreca",
    "repository": "https://github.com/rflagreca/vscode-clj-zprint",
    "license": "GPL-3.0-or-later",
    "engines": {
        "vscode": "^1.51.0"
    },
    "categories": [
        "Formatters"
    ],
    "activationEvents": [
        "onLanguage:clojure"
    ],
    "main": "./extension.js",
    "contributes": {
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "yarn run lint",
        "test": "node ./test/runTest.js",
        "clean": "rm -rf .shadow-cljs/ && rm extension.js",
        "compile": "yarn run shadow-cljs compile :extension",
        "watch": "yarn run shadow-cljs watch :extension",
        "release": "yarn run clean && yarn run shadow-cljs release :extension",
        "package": "yarn run release && vsce package"
    },
    "devDependencies": {
        "@types/vscode": "^1.51.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.0",
        "@types/node": "^12.11.7",
        "eslint": "^7.9.0",
        "glob": "^7.1.6",
        "mocha": "^8.1.3",
        "typescript": "^4.0.2",
        "vscode-test": "^1.4.0",
        "shadow-cljs": "^2.11.7"
    }
}
